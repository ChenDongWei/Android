<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.thxy.mobileguard.fragment.UnlockedFragment">

    <TextView
        android:id="@+id/tv_fragment_unlocked_lab"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="未加锁软件(12)"/>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ListView
            android:id="@+id/lv_fragment_unlocked_datas"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </ListView>

        <TextView
            android:id="@+id/tv_fragment_unlocked_listview_tag"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/grey"
            android:text="listViewtag"
            android:textColor="@color/white"/>

        <ProgressBar
            android:id="@+id/pb_fragment_unlocked_loading"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"/>
    </FrameLayout>
</LinearLayout>

<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="65dp"
    android:background="@color/gray"
    android:clickable="true">

    <TextView
        android:id="@+id/tv_telsmssafe_listview_item_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:textSize="20sp"/>

    <TextView
        android:id="@+id/tv_telsmssafe_listview_item_mode"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/tv_telsmssafe_listview_item_number"
        android:layout_margin="5dp"
        android:textSize="18sp"/>

    <ImageView
        android:id="@+id/iv_telsmssafe_imageview_item_delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:clickable="false"
        android:src="@drawable/iv_delete_selector"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="3dp"
        android:layout_below="@id/tv_settingcenter_autoupdate_content"
        android:background="@drawable/list_devider"/>
</RelativeLayout>
